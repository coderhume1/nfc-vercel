{
  "name": "nfc-pay-vercel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "if [ -n \"$DATABASE_URL\" ]; then npx prisma migrate deploy || npx prisma db push; else echo \"DATABASE_URL not set; skipping Prisma\"; fi && next build",
    "postinstall": "prisma generate",
    "start": "next start -p $PORT"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "prisma": "5.22.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "typescript": "^5.4.5",
    "tailwindcss": "3.4.10",
    "postcss": "8.4.41",
    "autoprefixer": "10.4.19"
  }
}